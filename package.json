  "engines": {
    "node": ">= 6"
  },
  "dependencies": {
    "fastify-plugin": "^1.0.1"
  },
  "devDependencies": {
    "autocannon": "^2.4.1",
    "ava": "^0.25.0",
    "coveralls": "^3.0.1",
    "dont-sniff-mimetype": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-handsome": "^0.5.0",
